        "**/.ipynb_checkpoints": true,
        ".ipynb_checkpoints": true,
        ".pytest_cache": true,
        ".mypy_cache": true,
        "*.egg-info": true,
        ".coverage": true,
        "**/htmlcov": true
    },
    "git.confirmSync": false,
    "workbench.editorAssociations": {
